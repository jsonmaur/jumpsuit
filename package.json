{
  "name": "jumpsuit",
  "version": "0.0.12",
  "description": "One javascript framework to rule them all",
  "homepage": "http://jumpsuit.io",
  "license": "MIT",
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zab/jumpsuit.git"
  },
  "main": "cli/lib/core.js",
  "bin": {
    "jumpsuit": "cli/bin/jumpsuit"
  },
  "files": [
    "cli/bin/",
    "cli/lib/",
    "cli/logo.txt",
    "banner.png",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test:lint": "exlint cli/src cli/test cli/bin",
    "test:unit": "nyc ava cli/test/unit -a",
    "test:integration": "ava cli/test/integration",
    "pretest": "rm -rf .nyc_output coverage",
    "test": "cross-env NODE_ENV=testing run-s test:*",
    "posttest": "nyc report -r=lcov",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "prebuild": "rm -rf cli/lib",
    "build": "babel cli/src -d cli/lib --no-comments",
    "watch": "npm run build -- -w -s",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-runtime": "6.6.1",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "browserify-transform-tools": "1.6.0",
    "butler": "0.4.0",
    "chalk": "1.1.3",
    "chokidar": "1.4.3",
    "clean-css": "3.4.12",
    "crypto-extra": "0.2.2",
    "exorcist": "^0.4.0",
    "forgetify": "0.1.2",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.3",
    "insert-css": "0.2.0",
    "jumpsuit-core": "0.0.11",
    "lodash": "4.12.0",
    "loose-envify": "1.1.0",
    "memory-fs": "0.3.0",
    "minimatch": "3.0.0",
    "minimist": "1.2.0",
    "postcss": "5.0.21",
    "postcss-import": "8.1.2",
    "postcss-modules": "0.4.1",
    "resolve": "1.1.7",
    "strip-ansi": "3.0.1",
    "uglify-js": "2.6.2",
    "uglifyify": "3.0.1",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "ava": "0.14.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-register": "6.8.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "exlint": "0.1.4",
    "npm-run-all": "2.1.1",
    "nyc": "6.4.4",
    "proxyquire": "1.7.8",
    "sinon": "1.17.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "include": [
      "cli/src"
    ]
  }
}
