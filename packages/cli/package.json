{
  "name": "jumpsuit-cli",
  "version": "0.7.1",
  "description": "Jump in. Zip up. Build great apps.",
  "license": "MIT",
  "homepage": "https://github.com/jumpsuit/jumpsuit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpsuit/jumpsuit.git"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "preferGlobal": true,
  "main": "lib/index.js",
  "bin": {
    "js": "bin/jumpsuit",
    "jumpsuit": "bin/jumpsuit"
  },
  "files": [
    "assets/",
    "bin/",
    "examples/",
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test:lint": "standard --verbose | snazzy",
    "test": "run-run-all test:*",
    "build": "rm -rf lib && babel src -d lib --no-comments",
    "watch": "npm run build -- -w -s",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "aliasify": "^2.0.0",
    "axios": "0.11.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-runtime": "6.6.1",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "browserify-transform-tools": "1.6.0",
    "butler": "0.4.1",
    "chalk": "1.1.3",
    "chokidar": "1.4.3",
    "clean-css": "3.4.12",
    "crypto-extra": "0.2.2",
    "exorcist": "^0.4.0",
    "forgetify": "0.1.2",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.3",
    "insert-css": "0.2.0",
    "jumpsuit": "^0.7.0",
    "lodash": "4.12.0",
    "loose-envify": "1.1.0",
    "memory-fs": "0.3.0",
    "minimatch": "3.0.0",
    "minimist": "1.2.0",
    "postcss": "5.0.21",
    "postcss-import": "8.1.2",
    "postcss-modules": "0.4.1",
    "query-string": "4.1.0",
    "resolve": "1.1.7",
    "strip-ansi": "3.0.1",
    "uglify-js": "2.6.2",
    "uglifyify": "3.0.1",
    "cross-spawn": "^4.0.0",
    "vinyl-transform": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-eslint": "^6.1.1",
    "babel-plugin-transform-runtime": "6.8.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "npm-run-all": "2.1.1",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
