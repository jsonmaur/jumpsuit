{
  "name": "jumpsuit-cli",
  "version": "1.0.5",
  "description": "Jump in. Zip up. Build great apps.",
  "license": "MIT",
  "homepage": "https://github.com/jumpsuit/jumpsuit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jumpsuit/jumpsuit.git"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=3"
  },
  "keywords": [
    "framework",
    "react",
    "redux",
    "react-router",
    "spa",
    "front-end"
  ],
  "preferGlobal": true,
  "main": "lib/index.js",
  "bin": {
    "js": "bin/jumpsuit",
    "jumpsuit": "bin/jumpsuit"
  },
  "files": [
    "assets/",
    "bin/",
    "getting-started/src",
    "getting-started/jumpsuit.config.js",
    "getting-started/package.json",
    "getting-started/README.md",
    "lib/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test:lint": "standard --verbose | snazzy",
    "test": "npm-run-all test:*",
    "build": "rm -rf lib && babel src -d lib --no-comments",
    "watch": "npm run build -- -w -s",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "aliasify": "2.0.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "6.13.0",
    "babel-runtime": "6.11.6",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "browserify-transform-tools": "1.6.0",
    "jumpserver": "^0.8.3",
    "chalk": "1.1.3",
    "chokidar": "1.6.0",
    "cross-spawn": "4.0.0",
    "deep-assign": "2.0.0",
    "exorcist": "0.4.0",
    "forgetify": "0.1.2",
    "fs-extra": "0.30.0",
    "fs-promise": "0.5.0",
    "glob": "7.0.6",
    "inquirer": "1.1.3",
    "jumpsuit": "^0.7.10",
    "loose-envify": "1.2.0",
    "minimatch": "3.0.3",
    "minimist": "1.2.0",
    "resolve": "1.1.7",
    "strip-ansi": "3.0.1",
    "uglify-js": "2.7.3",
    "uglifyify": "3.0.3",
    "update-notifier": "1.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-register": "6.14.0",
    "npm-run-all": "3.1.0",
    "snazzy": "5.0.0",
    "standard": "8.0.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib",
      "dist",
      "node_modules"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
